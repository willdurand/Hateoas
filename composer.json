{
    "name": "willdurand/hateoas",
    "description": "A PHP library to support implementing representations for HATEOAS REST web services",
    "license": "MIT",
    "authors": [
        {
            "name": "William DURAND",
            "email": "william.durand1@gmail.com"
        }
    ],
    "require": {
        "symfony/property-access": "~2.2",
        "jms/serializer": ">=0.13",
        "symfony/yaml": "~2.0"
    },
    "require-dev": {
        "symfony/routing": "~2.0",
        "mikey179/vfsStream": "~1.2"
    },
    "suggest": {
        "symfony/routing": "Required if you want to use the LinkBuilder"
    },
    "autoload": {
        "psr-0": { "Hateoas": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
